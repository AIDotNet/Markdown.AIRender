        <Setter Property="Padding" Value="3 10 1 1" />
        <Setter Property="Margin" Value="0 0 3 0" />
        <Setter Property="LineHeight" Value="26" />
    </Style>

    <Style Selector="SelectableTextBlock.MdH3_TechnologyBlue">
        <Setter Property="FontSize" Value="18" />
